{
  "name": "@heroku-cli/plugin-autocomplete",
  "version": "1.0.7",
  "author": "Philipe Navarro @RasPhilCo",
  "bugs": "https://github.com/heroku/heroku-cli-autocomplete/issues",
  "dependencies": {
    "@heroku-cli/command": "^8.1.24",
    "@oclif/command": "1.4.33",
    "@oclif/config": "1.6.28",
    "chalk": "^2.4.1",
    "cli-ux": "^4.6.3",
    "debug": "^3.1.0",
    "fs-extra": "^6.0.1",
    "lodash.flatten": "^4.4.0",
    "moment": "^2.22.2",
    "tslib": "1.9.2"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@oclif/dev-cli": "1.13.31",
    "@oclif/plugin-help": "2.0.5",
    "@oclif/test": "1.1.0",
    "@oclif/tslint": "1.1.2",
    "@types/chai": "4.1.4",
    "@types/fs-extra": "^5.0.3",
    "@types/lodash.flatten": "^4.4.3",
    "@types/mocha": "5.2.2",
    "@types/nock": "^9.1.3",
    "@types/node": "10.3.3",
    "chai": "4",
    "globby": "8",
    "mocha": "5.2.0",
    "nock": "^9.3.3",
    "nyc": "12.0.2",
    "ts-node": "6.1.1",
    "tslint": "5.10.0",
    "typescript": "2.9.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/.oclif.manifest.json",
    "autocomplete",
    "/lib",
    "yarn.lock"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-autocomplete",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "hooks": {
      "update": "./lib/hooks/recache",
      "recache": "./lib/hooks/recache"
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-autocomplete",
  "scripts": {
    "postpublish": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepublishOnly": "rm -rf lib && tsc && oclif-dev manifest",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\""
  }
}
